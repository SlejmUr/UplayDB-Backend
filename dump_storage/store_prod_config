root:
  dev:
    require_legacy_game_password: online_only
  name: "Far Cry® 3"
  background_image: f76fdfbd11e9e9252f2dd49dcaa0c4b9.jpg
  thumb_image: ed9da50536f11b289121329af97881e1.jpg
  logo_image: 4d3b3afc6ab3ed8a08b6caf52cb37a9e.png
  dialog_image: 9daa19d3b7dd717067174a81d5d22c9a.png
