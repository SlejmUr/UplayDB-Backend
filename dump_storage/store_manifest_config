version: 2.0
root:
  dev:
    require_legacy_game_password: always
  name: "Anno 2070"
  background_image: dab54ae00427d76aa3cf655c4f8c4dde.jpg
  thumb_image: 7270bb74bded62634b57878b7bcb3a22.jpg
  logo_image: 7a2177acf0ba33bbcfbee6ee35108bdb.png
  dialog_image: dfa4ff817377dd65f5cf04c091ee8cb9.png
  icon_image: fb2ebde69f7640f2ac8d00752b8a60bb.ico
  start_game:
    online:
      legacy_ticket_enabled: yes
      after_game_report_enabled: yes
      overlay_supported: no
      executables:
        - path: 
            relative: Anno5.exe
          arguments: -updatecomplete
          working_directory:
            register: HKEY_LOCAL_MACHINE\SOFTWARE\Ubisoft\ANNO 5\GameUpdate\installdir
          evil_updater:
            path: 
              relative: AutoPatcher.exe
            arguments: /autostart
            working_directory:
              register: HKEY_LOCAL_MACHINE\SOFTWARE\Ubisoft\ANNO 5\GameUpdate\installdir
    offline:
      after_game_report_enabled: yes
      executables:
        - path: 
            relative: Anno5.exe
          arguments: -offline
          working_directory:
            register: HKEY_LOCAL_MACHINE\SOFTWARE\Ubisoft\ANNO 5\GameUpdate\installdir
  installer:
